                    </td>
                    <td>
                        <%= mytable[iCount].occupationstatus %>
                    </td>
                    <td>
                        <%= mytable[iCount].location %>
                    </td>
                    <td>
                        <%= mytable[iCount].na %>
                    </td>
                    <td>
                        <%= mytable[iCount].private %>
                    </td>
                    <td>
                        <%= mytable[iCount].school %>
                    </td>
                    <td>
                        <%= mytable[iCount].university %>
                    </td>
                    <td>
                        <%= mytable[iCount].company %>
                    </td>
                    <td>
                        <%= mytable[iCount].government %>
                    </td>
                    <td>
                        <%= mytable[iCount].usesm %>
                    </td>
                    <td>
                        <%= mytable[iCount].facebook %>
                    </td>
                    <td>
