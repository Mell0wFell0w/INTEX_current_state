                    </td>
                    <td>
                        <%= mytable[iCount].q20 %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>

    <script>
        function searchTable() {
            let input, filter, table, tr, td, i, txtValue;
            input = document.getElementById("searchInput");
            filter = input.value.trim(); // Trim any whitespace
    
            table = document.getElementById("surveyTable");
            tr = table.getElementsByTagName("tr");
    
            for (i = 0; i < tr.length; i++) {
                td = tr[i].getElementsByTagName("td")[0]; // Change the index if your survey ID is in a different column
                if (td) {
                    txtValue = td.textContent || td.innerText;
                    if (txtValue.trim() === filter) { // Check for exact match
                        tr[i].style.display = "";
                    } else {
                        tr[i].style.display = "none";
                    }
                }
            }
        }
    </script>
    <div class="footer-content">
        <a href="/">Home</a>
        <a href="/edit">Edit login credentials</a>
    </div>
</body>

</html>